{
  "name": "web-fullstack-engineer",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "yarn install; tsc; pm2 start pm2.json --env=local; concurrently \"tsc --watch\" \"pm2 logs web-fullstack-engineer\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "jest --coverage -t=unit",
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json --fix"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/jest": "22.2.0",
    "@types/node": "8.10.9",
    "concurrently": "3.5.1",
    "jest": "22.4.3",
    "pm2": "3.1.2",
    "ts-jest": "23.10.4",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "aws-sdk": "2.347.0",
    "class-validator": "0.9.1",
    "express": "4.16.0",
    "express-async-router": "0.1.13",
    "reflect-metadata": "0.1.12",
    "typeorm": "0.2.6"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
